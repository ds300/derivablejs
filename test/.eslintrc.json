{
  "env": {
    "jest": true
  },
  "rules": {
    "no-global-assign": 0,
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "no-shadow": "error",
    "prefer-const": "error",
    "es5/no-block-scoping": 0,
    "es5/no-arrow-functions": 0,
    "es5/no-template-literals": 0,
    "es5/no-rest-parameters": 0,
    "es5/no-shorthand-properties": 0
  }
}
